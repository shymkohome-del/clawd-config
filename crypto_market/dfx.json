{
  "canisters": {
    "user_management": {
      "main": "canisters/user_management/src/main_simple.mo",
      "type": "motoko",
      "candid": "canisters/user_management/user_management.did",
      "declarations": {
        "output": "lib/core/blockchain/declarations/user_management"
      }
    },
    "marketplace": {
      "main": "canisters/marketplace/src/main_simple.mo",
      "type": "motoko",
      "candid": "canisters/marketplace/marketplace.did",
      "declarations": {
        "output": "lib/core/blockchain/declarations/marketplace"
      }
    },
    "atomic_swap": {
      "main": "canisters/atomic_swap/src/main_simple.mo",
      "type": "motoko",
      "candid": "canisters/atomic_swap/atomic_swap.did",
      "declarations": {
        "output": "lib/core/blockchain/declarations/atomic_swap"
      }
    },
    "price_oracle": {
      "main": "canisters/price_oracle/src/main_simple.mo",
      "type": "motoko",
      "candid": "canisters/price_oracle/price_oracle.did",
      "declarations": {
        "output": "lib/core/blockchain/declarations/price_oracle"
      }
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    },
    "replica": {
      "subnet_type": "system"
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "providers": ["https://ic0.app"],
      "type": "persistent"
    }
  },
  "version": 1
}
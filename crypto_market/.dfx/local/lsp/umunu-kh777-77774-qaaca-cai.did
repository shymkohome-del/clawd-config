type User = 
 record {
   createdAt: int;
   email: text;
   id: principal;
   username: text;
 };
type RegisterResult = 
 variant {
   err: text;
   ok: User;
 };
service : {
  getUser: (id: principal) -> (opt User) query;
  healthCheck: () -> (bool) query;
  register: (email: text, username: text) -> (RegisterResult);
}

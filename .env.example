# Copy this file to .env and fill with real values for local development.
# Do NOT commit real secrets.

# OAuth (Google)
OAUTH_GOOGLE_CLIENT_ID=
OAUTH_GOOGLE_CLIENT_SECRET=

# OAuth (Apple) - placeholders OK for early dev
OAUTH_APPLE_TEAM_ID=dev-placeholder
OAUTH_APPLE_KEY_ID=dev-placeholder

# IPFS (use Infura or local node)
# Example Infura node URL: https://ipfs.infura.io:5001
# Example public gateway: https://ipfs.io/ipfs/
IPFS_NODE_URL=https://ipfs.infura.io:5001
IPFS_GATEWAY_URL=https://ipfs.io/ipfs/

# Internet Computer canister IDs (use real IDs from dfx deploy or placeholders)
CANISTER_ID_MARKETPLACE=aaaaa-aa
CANISTER_ID_USER_MANAGEMENT=bbbbb-bb
CANISTER_ID_ATOMIC_SWAP=ccccc-cc
CANISTER_ID_PRICE_ORACLE=ddddd-dd

# Optional API keys
CHAINLINK_API_KEY=
COINGECKO_API_KEY=
KYC_PROVIDER_API_KEY=
